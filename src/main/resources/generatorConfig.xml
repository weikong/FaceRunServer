        <table tableName="petproductfood" domainObjectName="PetProductFood">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>
        <table tableName="petcompany" domainObjectName="PetCompany">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
        </table>
        <table tableName="food" domainObjectName="Food">
            <generatedKey column="id" sqlStatement="MySql" identity="true"/>
